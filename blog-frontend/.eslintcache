g & Set Specific Post Data\r\n  useEffect(() => {\r\n    if (id) {\r\n      getPostData();\r\n    }\r\n  }, []);\r\n\r\n  const submitHandle = (e) => {\r\n    e.preventDefault();\r\n    console.log(\"this\");\r\n    let formData = new FormData();\r\n\r\n    formData.append(\"title\", title);\r\n    formData.append(\"body\", body);\r\n    formData.append(\"file\", image);\r\n\r\n    if (!id) {\r\n      dispatch(addPost(formData));\r\n      history.push(\"/admin\");\r\n    } else {\r\n      dispatch(updatePost(id, formData));\r\n      history.push(\"/admin\");\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"form_container shadow\">\r\n      <form onSubmit={(e) => submitHandle(e)}>\r\n        <div className=\"form-group\">\r\n          <label>Enter title of the post : </label>\r\n          <input\r\n            type=\"text\"\r\n            name=\"title\"\r\n            className=\"form-control\"\r\n            onChange={(e) => setTitle(e.target.value)}\r\n            value={title}\r\n          />\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <label htmlFor=\"\">Enter your post description :</label>\r\n          <CKEditor data={body} onChange={(e) => setBody(e.editor.getData())} />\r\n        </div>\r\n        <div className=\"form-group\">\r\n          <label htmlFor=\"\">Upload Image :</label>\r\n          <input type=\"file\" onChange={(e) => handleImage(e)} />\r\n        </div>\r\n        {id ? <img src={imageUrl} alt=\"post-img\" className=\"w-50\" /> : \"\"}\r\n        <div className=\"text-center\">\r\n          <button type=\"submit\" className=\"btn btn-primary\">\r\n            {id ? \"Update Post\" : \"Add Post\"}\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default PostForm;\r\n","E:\\04 Website Learning\\MERN STACK\\react-node-blog\\blog-frontend\\src\\reducers\\UserReducer.js",[],"E:\\04 Website Learning\\MERN STACK\\react-node-blog\\blog-frontend\\src\\reducers\\Er